{
    "name": "dragosprotung/fitness-tracker-porting",
    "description": "A set of tools to dump / upload / port workouts from different online fitness trackers to another (ex endomondo.com, flow.polar.com).",
    "keywords": ["endomondo", "polar", "fitness", "portal"],
    "homepage": "http://github.com/dragosprotung/FitnessTrackingPorting",
    "license": "MIT",
    "authors": [
        {
            "name": "Dragos Protung",
            "email": "dragos@protung.ro"
        }
    ],
    "require": {
        "php": ">=5.4",
        "symfony/console": "2.5.*",
        "symfony/yaml": "2.5.*",
        "guzzlehttp/guzzle": "4.*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.1.*"
    },
    "autoload": {
        "psr-0": {
            "FitnessTrackingPorting\\": "src/"
        }
    }
}
